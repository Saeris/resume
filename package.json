{
  "name": "@saeris/resume",
  "description": "Personal Resume",
  "version": "2023.05.23",
  "author": "Drake Costa <drake@saeris.io> (https://saeris.gg/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/saeris/resume.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "yarn lint --fix",
    "lint": "eslint \"./src/**/*.{j,t}s?(x)\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/mdx": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "^13.4.3",
    "@types/mdx": "^2.0.5",
    "next": "^13.4.3",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "typeface-orbitron": "^1.1.13",
    "typeface-titillium-web": "^1.1.13"
  },
  "devDependencies": {
    "@saeris/eslint-config": "^2.3.1",
    "@types/node": "18.16.13",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-plugin-import": "^2.27.5",
    "postcss": "^8.4.23",
    "postcss-custom-media": "^9.1.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^8.4.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "packageManager": "yarn@3.2.1",
  "eslintConfig": {
    "extends": [
      "@saeris/eslint-config/base",
      "@saeris/eslint-config/react",
      "@saeris/eslint-config/next",
      "@saeris/eslint-config/typescript",
      "@saeris/eslint-config/type-aware"
    ],
    "ignorePatterns": [
      "*.js",
      "*.mjs",
      "*.d.ts"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false
  }
}
